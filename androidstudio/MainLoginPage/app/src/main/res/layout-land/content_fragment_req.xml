<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".FragmentReqActivity"
    tools:showIn="@layout/app_bar_fragment_req">

    <fragment
        android:id="@+id/mainlayout"
        android:name="com.example.mainloginpage.MyRequestFragment"
        android:layout_width="340dp"
        android:layout_height="362dp"
        android:layout_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <fragment
        android:id="@+id/reqviewlandscape"
        android:name="com.example.mainloginpage.ReqViewFragment"
        android:layout_width="373dp"
        android:layout_height="365dp"
        android:layout_marginEnd="8dp"
        android:layout_weight="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>